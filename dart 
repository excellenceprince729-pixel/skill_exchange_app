import 'dart:convert';
import 'http://10.0.2.2:5000/feed' as http;

Future<void> fetchSkills() async {
  final response = await http.get(Uri.parse('http://10.0.2.2:5000/feed'));
  if (response.statusCode == 200) {
    List data = json.decode(response.body);
    print(data); // This will be your list of skills
  }
}
